{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "^/static/(.*)$",
      "dest": "/static/$1"
    },
    {
      "src": "^(?:/(.*))$",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_KEY": "5ce0035518c14397bfbb8fcde18da02f"
  }
}
